#!/usr/bin/env php
<?php
spl_autoload_register(
  function ($class)
  {
      static $classes = NULL;

      if ($classes === NULL) {
          $classes = array(
            ___CLASSLIST___
          );
      }

      $class = strtolower($class);

      if (isset($classes[$class])) {
          require 'phar://phpdox.phar/' . $classes[$class];
      }
  }
);

Phar::mapPhar('phpdox.phar');
define('PHPDOX_VERSION', '___VERSION___');
define('PHPDOX_PHAR', 'phpdox.phar');
$factory = new \TheSeer\phpDox\Factory();
$factory->getInstanceFor('CLI')->run(new \TheSeer\phpDox\CLIOptions($_SERVER['argv']));
exit(0);

__HALT_COMPILER();
